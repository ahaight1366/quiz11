
CREATE TABLE public.answers
(
    ansid bigint NOT NULL GENERATED ALWAYS AS IDENTITY ( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 9223372036854775807 CACHE 1 ),
    groupname text COLLATE pg_catalog."default",
    username text COLLATE pg_catalog."default",
    ans text COLLATE pg_catalog."default",
    CONSTRAINT answers_pkey PRIMARY KEY (ansid)
)

TABLESPACE pg_default;

ALTER TABLE public.answers
    OWNER to fmhwoxcvqiwsyq;



-- Table: public.questions

-- DROP TABLE public.questions;

CREATE TABLE public.questions
(
    "questionID" bigint NOT NULL GENERATED BY DEFAULT AS IDENTITY ( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 9223372036854775807 CACHE 1 ),
    groupname text COLLATE pg_catalog."default",
    userno smallint,
    correctanswer text COLLATE pg_catalog."default",
    difficulty text COLLATE pg_catalog."default",
    question text COLLATE pg_catalog."default",
    answers text[] COLLATE pg_catalog."default",
    quesno smallint,
    CONSTRAINT questions_pkey PRIMARY KEY ("questionID")
)

TABLESPACE pg_default;

ALTER TABLE public.questions
    OWNER to postgres;
COMMENT ON TABLE public.questions
    IS 'question collection of  any room';